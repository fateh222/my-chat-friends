<div class="container p-3">
  <h2> All Users </h2>
  <hr>
  <div class="list-group">
    <% @users.each do | user | %>
      <% if user != current_user %>
        <%= link_to user.email, "chat/user/#{user.id}", class: "list-group-item list-group-item-action" %>
      <% end %>
    <% end %>
  </div>

</div>
